[user]
	name = Wagner Augusto Andreoli
	email = wagnerandreoli@gmail.com
[alias]
	br = branch
	st = status
	co = checkout
	ci = commit
	bs = "!git br ; git stash list ; git st"
[credential]
	helper = osxkeychain
[push]
	default = simple
[diff]
	tool = opendiff
	external = git-external-diff.sh
[difftool]
	prompt = no
[color]
	branch = auto
	diff = auto
	grep = auto
	interactive = auto
	status = auto
	showbranch = auto
	ui = auto
[giggle]
	main-window-maximized = true
	main-window-geometry = 700x550+0+55
	history-view-vpane-position = 345
	main-window-view = HistoryView
	file-view-hpane-position = 209
	file-view-vpane-position = 846
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[status]
	showUntrackedFiles = all
[github]
	user = wagnerandreoli
	token = 6e0f54783d050d53662979824c994bff
[core]
	editor = subl -w
[log]
	date = iso
